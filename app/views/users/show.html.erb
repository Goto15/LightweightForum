<h1><%= @user.username %></h1>
<div class='author'><%= link_to "Edit profile" , edit_user_path(@user) %></div>
<div class='topics'>Topics:</div>
<ul>
    <div class='topics'>
    <% @topics.each do |topic| %>
        <li>
            <%= link_to topic.name, topic_path(topic) %>
        </li>
    <% end %>
    </div>
</ul>